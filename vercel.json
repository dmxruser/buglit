{
  "version": 2,
  "functions": {
    "api/index.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/login/github",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/auth/github/callback",
      "dest": "/api/index.py"
    },
    {
      "src": "/auth/github/callback",
      "dest": "/api/index.py"
    },
    {
      "src": "/user/repos",
      "dest": "/api/index.py"
    },
    {
      "src": "/issues",
      "dest": "/api/index.py"
    },
    {
      "src": "/run-command",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/v1/gemini-cron",
      "schedule": "30 1 * * *"
    }
  ]
}